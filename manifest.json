{
  "manifest_version": 2,

  "name": "Two Things",
  "description": "Make it easy to generate tinyletter blog updates",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage"
  ]
}
